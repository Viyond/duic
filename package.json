    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-typescript": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "assets-webpack-plugin": "^3.9.10",
    "clean-webpack-plugin": "^3.0.0",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "stylint": "^2.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
